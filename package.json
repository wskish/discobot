{
    "name": "discobot",
    "version": "0.0.0-dev",
    "private": true,
    "type": "module",
    "scripts": {
        "build": "vite build",
        "dev": "concurrently -n vite,server,agent -c cyan,green,magenta \"pnpm dev:vite\" \"pnpm dev:server\" \"pnpm dev:agent\"",
        "dev:vite": "vite",
        "dev:server": "cd server && cross-env SUGGESTIONS_ENABLED=true go run github.com/air-verse/air@latest",
        "dev:agent": "tsx --watch scripts/agent-watcher/index.ts",
        "dev:vz": "tsx --watch scripts/vz-watcher/index.ts",
        "dev:tauri": "vite --port 5173",
        "build:vite": "vite build",
        "build:server": "node scripts/build-server.mjs",
        "preview": "vite preview",
        "tauri": "tauri",
        "test": "pnpm test:unit && pnpm test:integration",
        "test:unit": "pnpm test:server:unit && pnpm test:proxy:unit && pnpm test:agent:unit && pnpm test:watcher && pnpm test:hooks",
        "test:hooks": "node --test lib/hooks/use-messages.test.ts lib/hooks/use-preferences.test.ts lib/hooks/use-prompt-history.test.ts",
        "test:integration": "pnpm test:server:integration && pnpm test:proxy:integration && pnpm test:agent:integration",
        "test:server": "cd server && go test -v ./...",
        "test:server:unit": "cd server && go test -v $(go list ./... | grep -v /integration)",
        "test:server:integration": "cd server && go test -v ./internal/integration/...",
        "test:proxy": "cd proxy && go test -v ./...",
        "test:proxy:unit": "cd proxy && go test -v $(go list ./... | grep -v /integration)",
        "test:proxy:integration": "cd proxy && go test -v -run Integration ./internal/proxy/",
        "test:agent": "pnpm --filter agent-api test",
        "test:agent:unit": "pnpm --filter agent-api test:unit",
        "test:agent:integration": "pnpm --filter agent-api test:integration",
        "test:watcher": "node --import tsx --test scripts/agent-watcher/watcher.test.ts",
        "ci": "pnpm check:fix && pnpm test:unit && pnpm build && ([ \"$CI\" = \"true\" ] && git diff --exit-code || true)",
        "lint": "biome lint . && pnpm lint:go && pnpm lint:react-compiler",
        "lint:go": "pnpm lint:go:server && pnpm lint:go:proxy",
        "lint:go:server": "cd server && go tool golangci-lint run",
        "lint:go:proxy": "cd proxy && go tool golangci-lint run",
        "lint:react-compiler": "eslint .",
        "format": "biome format . --write",
        "check": "biome check . && pnpm lint:go && pnpm lint:react-compiler && pnpm typecheck",
        "check:fix": "biome check . --write && pnpm lint:go && pnpm lint:react-compiler && pnpm typecheck",
        "check:skills": ".claude/scripts/validate-skills.sh",
        "typecheck": "tsc --noEmit",
        "refresh-models": "./scripts/refresh-models-data.sh"
    },
    "dependencies": {
