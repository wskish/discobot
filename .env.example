# Local development environment variables
# Copy this file to .env and fill in your values

# React Dev Tools (optional)
# Set this to the HTTP URL of your React Dev Tools server to enable it
# Example: http://localhost:8097
REACT_DEVTOOLS_URL=http://localhost:8097

# Enable suggesting completions for files on the local directory
SUGGESTIONS_ENABLED=true

# CORS origins for local development
CORS_ORIGINS=http://localhost:3000,http://localhost:3001,http://*.localhost:3000,http://*.localhost:3001